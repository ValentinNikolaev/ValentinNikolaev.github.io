function CollapsePlaylist(elem){
	var dataId = $(elem).data("id");
	//alert(dataId);
	//$("collapseplaylist
	//$("#"+dataId).toggle("collapseplaylist");
	
	$("#"+dataId).toggleClass('collapseplaylist');
	if($("#"+dataId).hasClass('collapseplaylist')){
		$("#"+dataId).hide();
		$(".mylearning-"+dataId).addClass("out"); 
	} else{
		$("#"+dataId).show();
		$(".mylearning-"+dataId).removeClass("out");
	}
}

